{
  "name": "angui-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c hot",
    "build": "yarn run build:lib && yarn run build:app",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist && mkdir dist",
    "build:lib": "lerna list -a --toposort --json | node ./scripts/lerna-build-lib.js",
    "build:app": "ng build angui-app",
    "build:app:prod": "yarn build:app -c production",
    "build:prod": "yarn run build:lib && yarn run build:app:prod",
    "serve:dist": "http-server ./dist/angui-app -p 4602"
  },
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "core-js": "~3.6.4",
    "core-js-pure": "~3.6.4",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular-devkit/build-ng-packagr": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "http-server": "^0.12.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "lerna": "^3.20.2",
    "ng-packagr": "^10.1.1",
    "prettier": "~1.19.1",
    "protractor": "~5.4.2",
    "rimraf": "^3.0.0",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "typescript": "~3.8.3"
  }
